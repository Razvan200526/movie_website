{
  "name": "package.json",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:backend": "cd backend && bun run --watch src/index.ts",
    "dev:frontend": "vite",
    "dev": "concurrently \"bun run dev:backend\" \"bun run dev:frontend\"",
    "build:frontend": "vite build",
    "build:backend": "cd backend && bun build src/index.ts --outdir dist",
    "build": "bun run build:frontend && bun run build:backend",
    "start:backend": "cd backend && bun --bun run src/index.ts",
    "start:frontend": "vite preview",
    "start": "concurrently \"bun run start:backend\" \"bun run start:frontend\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@heroui/react": "^2.7.11",
    "@mui/material": "^7.2.0",
    "@sentry/bun": "^9.35.0",
    "@sentry/react": "^9.35.0",
    "@sentry/replay": "^7.116.0",
    "@sentry/tracing": "^7.120.3",
    "@sentry/vite-plugin": "^4.0.0",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-query-devtools": "^5.81.5",
    "@types/bcryptjs": "^3.0.0",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "all": "^0.0.0",
    "concurrently": "^9.2.0",
    "dotenv": "^17.2.0",
    "jsonwebtoken": "^9.0.2",
    "mui": "^0.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3",
    "react-spinners": "^0.17.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tailwindcss": "^4.1.11",
    "zod": "^3.25.76",
    "util" : "0.12.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0"
  }
}
